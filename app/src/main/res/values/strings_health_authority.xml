<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

  <!-- Health authority app title. -->
  <string name="app_title">@string/enx_appTitle</string>

  <string name="health_authority_id">@string/enx_healthAuthorityID</string>

  <string name="health_authority_name">@string/enx_agencyDisplayName</string>

  <!-- Health authority name displayed on app first open. -->
  <string name="onboarding_agency_name">@string/enx_agencyDisplayName</string>

  <!-- Health authority region name displayed on app first open. -->
  <string name="onboarding_region_name">@string/enx_agencyRegionName</string>

  <!-- Health authority legal subtitle on app first open. -->
  <string name="onboarding_legal_subtitle">@string/enx_agencyDisplayName</string>

  <!-- Health authority message displayed on app first open. -->
  <string name="onboarding_agency_message">@string/enx_agencyMessage</string>

  <!-- Health authority legal title, displayed in the onboarding flow, above the health authority's legal text. -->
  <string name="onboarding_legal_title">@string/enx_agencyRegionName</string>

  <!-- Displayed during the onboarding flow, showing the health authority's legal text. -->
  <string name="onboarding_legal_detail">@string/enx_legalConsentText</string>

  <string name="onboarding_welcome_text">@string/enx_agencyDisplayName</string>

  <string name="onboarding_start_text">@string/enx_agencyMessage</string>

  <!-- Displayed during the onboarding flow, showing the health authority's private analytics consent text. -->
  <string name="onboarding_private_analytics_consent_text">@string/enx_enpaConsentText</string>

  <!-- Name of the agency shown in the text of the exposure about screen. -->
  <string name="exposure_about_agency">@string/enx_agencyDisplayName</string>

  <!-- Title of the exposure notification when an exposure of classification 1 has been detected. -->
  <string name="exposure_notification_title_1">@string/enx_notificationSubject_1</string>

  <!-- Title of the exposure notification when an exposure of classification 2 has been detected. -->
  <string name="exposure_notification_title_2">@string/enx_notificationSubject_2</string>

  <!-- Title of the exposure notification when an exposure of classification 3 has been detected. -->
  <string name="exposure_notification_title_3">@string/enx_notificationSubject_3</string>

  <!-- Title of the exposure notification when an exposure of classification 4 has been detected. -->
  <string name="exposure_notification_title_4">@string/enx_notificationSubject_4</string>

  <!-- Title of the exposure notification when it has been detected that the exposure status has been revoked. -->
  <string name="exposure_notification_title_revoked">@string/enx_revokedNotificationSubject</string>

  <!-- Body text of the exposure notification when an exposure of classification 1 has been detected. -->
  <string name="exposure_notification_message_1">@string/enx_notificationBody_1</string>

  <!-- Body text of the exposure notification when an exposure of classification 2 has been detected. -->
  <string name="exposure_notification_message_2">@string/enx_notificationBody_2</string>

  <!-- Body text of the exposure notification when an exposure of classification 3 has been detected. -->
  <string name="exposure_notification_message_3">@string/enx_notificationBody_3</string>

  <!-- Body text of the exposure notification when an exposure of classification 4 has been detected. -->
  <string name="exposure_notification_message_4">@string/enx_notificationBody_4</string>

  <!-- Body text of the exposure notification when it has been detected that the exposure status has been revoked. -->
  <string name="exposure_notification_message_revoked">@string/enx_revokedNotificationBody</string>

  <!-- Text shown on the exposure details page for exposures of classification 1. -->
  <string name="exposure_details_text_1">@string/enx_exposureDetailsBodyText_1</string>

  <!-- Text shown on the exposure details page for exposures of classification 2. -->
  <string name="exposure_details_text_2">@string/enx_exposureDetailsBodyText_2</string>

  <!-- Text shown on the exposure details page for exposures of classification 3. -->
  <string name="exposure_details_text_3">@string/enx_exposureDetailsBodyText_3</string>

  <!-- Text shown on the exposure details page for exposures of classification 4. -->
  <string name="exposure_details_text_4">@string/enx_exposureDetailsBodyText_4</string>

  <!-- Text shown on the exposure details page for revoked exposures. -->
  <string name="exposure_details_text_revoked">@string/enx_revokedDetailsBodyText</string>

  <!-- Health authority URL for more information, shown on the exposure details page for exposures of classification 1. -->
  <string name="exposure_details_url_1">@string/enx_classificationURL_1</string>

  <!-- Health authority URL for more information, shown on the exposure details page for exposures of classification 2. -->
  <string name="exposure_details_url_2">@string/enx_classificationURL_2</string>

  <!-- Health authority URL for more information, shown on the exposure details page for exposures of classification 3. -->
  <string name="exposure_details_url_3">@string/enx_classificationURL_3</string>

  <!-- Health authority URL for more information, shown on the exposure details page for exposures of classification 4. -->
  <string name="exposure_details_url_4">@string/enx_classificationURL_4</string>

  <!-- Health authority URL for more information, shown on the exposure details page for revoked exposures. -->
  <string name="exposure_details_url_revoked">@string/enx_revokedURL</string>

  <!-- Health authority name to be displayed above their text at the beginning of the notify others flow. -->
  <string name="share_begin_agency_name">@string/enx_agencyDisplayName</string>

  <!-- Health authority region name to be displayed above their text at the beginning of the notify others flow. -->
  <string name="share_begin_agency_region">@string/enx_agencyRegionName</string>

  <!-- Health authority text to be displayed at the begining of the notify others flow. -->
  <string name="share_begin_detail">@string/enx_testVerificationIntroMessage</string>

  <!-- URL for the learn more link on the share verification code page. -->
  <string name="share_verification_code_learn_more_url">@string/enx_verificationCodeLearnMoreURL</string>

  <!-- Text displayed on the third screen in the notify others flow, describing COVID-19 symptom onset. -->
  <string name="share_onset_detail">@string/enx_symptomsOnsetDescription</string>

  <!-- Text displayed on the fourth screen in the notify others flow, describing the question of whether the user has traveled. -->
  <string name="share_travel_detail">@string/enx_traveledQuestionText</string>

  <!-- Text displayed on the fifth screen in the notify others flow, describing the question of whether the user has been vaccinated. -->
  <string name="share_vaccination_detail">@string/enx_vaccinationQuestionText</string>

  <!-- Name for public health authority website link. -->
  <string name="health_authority_website_name">@string/enx_agencyDisplayName</string>

  <!-- HTTP URL for the public health authority website. -->
  <string name="health_authority_website_url">@string/enx_agencyWebsiteURL</string>

  <!-- Title of the agency message page found from the settings page. -->
  <string name="agency_message_title">@string/enx_agencyDisplayName</string>

  <!-- Subtitle on the agency message page, shows the agency's region name. -->
  <string name="agency_message_subtitle_region">@string/enx_agencyRegionName</string>

  <!-- Subtitle on the agency message page, shows the agency's name. -->
  <string name="agency_message_subtitle_name">@string/enx_agencyDisplayName</string>

  <!-- Main body of text on the agency message page, showing the agency's message. -->
  <string name="agency_message_detail">@string/enx_agencyMessage</string>

  <!-- Subtitle on the legal terms page, shows the agency's region name. -->
  <string name="legal_terms_subtitle_region">@string/enx_agencyRegionName</string>

  <!-- Subtitle on the legal terms page, shows the agency's name. -->
  <string name="legal_terms_subtitle_name">@string/enx_agencyDisplayName</string>

  <!-- Main body of text on the legal terms page, showing the agency's legal content. -->
  <string name="legal_terms_detail">@string/enx_legalConsentText</string>

  <!-- Name of the agency shown to be put into error messages. -->
  <string name="error_agency_name">@string/enx_agencyDisplayName</string>

  <!-- Text shown on the private analytics toggle consent page. -->
  <string name="private_analytics_message">@string/enx_enpaConsentText</string>

  <!-- HA-provided URL for self-reporting. If this URL is empty, it means that HA has not enabled self-report. -->
  <string name="self_report_website_url">@string/enx_webReportURL</string>

  <!-- Text shown on the "Did you receive a code?" screen of the upload flow to provide user with information on the verification codes. This screen is displayed only if Self-Report is enabled. -->
  <string name="self_report_intro">@string/enx_webReportIntroMessage</string>

  <!-- Text shown on the "Get a verification code" screen of the upload flow to provide user with information on the phone number they provide to request a code. This screen is displayed only if Self-Report is enabled. -->
  <string name="self_report_phone_number">@string/enx_webReportPhoneNumberMessage</string>

  <!-- Text shown on the "Get a verification code" screen of the upload flow to provide user with information on the test date they provide to request a code. This screen is displayed only if Self-Report is enabled. -->
  <string name="self_report_test_date">@string/enx_webReportDateMessage</string>

  <!-- Checkbox text shown on the "Get a verification code" screen of the upload flow. This checkbox is for users to confirm that they have had a positive COVID-19 test within the past 14 days. This screen is displayed only if Self-Report is enabled. -->
  <string name="self_report_checkbox">@string/enx_webReportLegalMessage</string>

  <!-- Message provided by Health Authority in case of the Exposure Notifications being turned down by this Health Authority. For V2 apps, this message is shown on the home screen and in the upload flow. For V3 apps, it is shown in the turndown notice and in the upload flow. -->
  <string name="turndown_agency_message">@string/enx_agencyTurndownMessage</string>

</resources>
